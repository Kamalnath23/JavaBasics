package tutorial17;

class Frog
{
	String name;
	int age;
	
	public void setName(String newName) {
		name=newName;
	}
	
	public String getName() {
		return name;
	}
	
	public int getAge() {
		return age;
	}
}

public class Setters {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Frog frog1= new Frog();{
			//frog1.name="Bertie";
			//frog1.age=1;
			
			frog1.setName("Bertie");
			
			System.out.println(frog1.getName());
		}

	}

}
